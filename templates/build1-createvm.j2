{% for node in m.cluster.nodes %}
	
sudo {{m.config.create_vm_script}} --name {{node.vmname}} --vcpu {{node.features.vcpu}} --ram {{node.features.memory}} --host {{node.host}} --disk {{node.features.root_size}} \
  --volume {{node.root_volume}} --hostname  {{node.hostname}} --buildkey {{m.config.keys_location}}/build_key --hostkey {{m.config.keys_location}}/host_key \
  --bridge {{node.network.bridge}} --ip {{node.ip}} --netmask {{node.network.netmask}} --gateway {{node.network.gateway}} --mac {{node.mac}} --dns {{node.network.dns}} &
	
{% endfor %}



